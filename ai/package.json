{
  "name": "Flutas_AI",
  "version": "4.1.0",
  "description": "ChatGPT helps User To Writeble Codings,...",
  "main": "main.js",
  "author": "Xbodw",
  "license": "MIT",
  "devDependencies": {
    "clipboard": "^2.0.11",
    "electron": "^19.0.0",
    "fs": "0.0.1-security"
  },
  "scripts": {
    "test": "@echo off && echo \"Error: no test specified\" && echo on && exit 1",
    "start": "@echo off && electron . && echo on",
    "build": "electron-builder --win --ia32",
    "package": "electron-packager ./ Flutas_AI --out ./Release -electron-version 19.0.0 -app-version 4.0.0 --overwrite --icon=favicon.ico"
  },
  "build": {
    "productName": "Flutas AI",
    "appId": "Flutas AI",
    "copyright": "Flutas",
    "compression": "store",
    "directories": {
      "output": "Release"
    },
    "asar": true,
    "extraResources": {
      "from": "./app-update.yml",
      "to": "./b.txt"
    },
    "win": {
      "icon": "favicon.ico",
      "extraResources": {
        "from": "./app-update.yml",
        "to": "./b.txt"
      }
    },
    "nsis": {
      "oneClick": false,
      "guid": "",
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./favicon.ico",
      "uninstallerIcon": "./favicon.ico",
      "installerHeaderIcon": "./favicon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Flutas AI"
    }
  },
  "dependencies": {
    "html-to-image": "^1.11.11",
    "sweetalert2": "^11.7.28"
  }
}
